{
    "name": "mdhedayet/appsumo-licensing",
    "description": "Laravel package to integrate with AppSumo Licensing automatically",
    "type": "library",
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "MdHedayet\\AppSumo-Licensing\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "MdHedayet\\AppSumo-Licensing\\AppSumoServiceProvider"
        ]
      }
    },
    "require": {
      "php": ">=7.3 || ^8.0",
      "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
      "guzzlehttp/guzzle": "^7.0"
    }
  }
  